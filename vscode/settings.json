{
    "workbench.iconTheme": "chalice-icon-theme",
    "vim.useCtrlKeys": false,
    "explorer.confirmDragAndDrop": false,
    "git.enableSmartCommit": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "markdown-preview-enhanced.previewTheme": "solarized-dark.css",
    "terminal.integrated.shell.windows": "cmd.exe",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "editor.suggestSelection": "first",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.tabSize": 2,
    "editor.cursorStyle": "line",
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "security.workspace.trust.untrustedFiles": "open",
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "editor.minimap.enabled": false,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": true,
        "markdown": true
    },
    "git.confirmSync": false,
    "window.zoomLevel": -0.04551739772644625,
    "files.autoSave": "onFocusChange",
    "workbench.colorTheme": "chalice-color-theme-dark",
    "blockman.n01LineHeight": 0,
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#1073cf2d",
        "editor.lineHighlightBorder": "#9fced11f"
    },
    "diffEditor.wordWrap": "off",
    "editor.guides.indentation": false,
    "editor.guides.bracketPairs": false,
    "editor.inlayHints.enabled": false,
    "blockman.n04ColorComboPreset": "Classic Dark 1 (Super gradients)",
    "blockman.n16EnableFocus": false,
    "eslint.alwaysShowStatus": true,
    "gitlens.plusFeatures.enabled": false,
    "gitlens.currentLine.pullRequests.enabled": false,
    "redhat.telemetry.enabled": true,
    "explorer.compactFolders": false,
    "vim.digraphs": {
    
    },
    "workbench.sideBar.location": "right",
    "jestrunner.configPath": "src/setupTests.ts",
    "explorer.confirmDelete": false,
    "editor.fontSize": 16,
    "vim.useSystemClipboard": true,
    "vva.trustedDomain": true,
    "editor.fontFamily": "'Hack', monospace",
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "remote.SSH.maxReconnectionAttempts": 8,
    "remote.SSH.remotePlatform": {
        "192.168.0.162": "windows"
    },
    "editor.fontLigatures": false,
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.go.organizeImports": true,
            "source.go.fixAllInFile": true,
            "source.go.sortImports": true
        }
    },
    "go.useLanguageServer": true,
    "go.alternateTools": {
        "go-langserver": "gopls"
    },
    "go.languageServerExperimentalFeatures": {
        "format": true,
        "autoComplete": true,
        "goToDefinition": true,
        "hover": true,
        "signatureHelp": true,
        "goToTypeDefinition": true
    },
    "go.buildOnSave": "off",
    "go.vetOnSave": "off",
    "go.autocompleteUnimportedPackages": true
}
